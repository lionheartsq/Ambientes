{% extends '::base.html.twig' %}

{% block body -%}
    <h1>CalendarioEvento</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Idcalendario</th>
                <td>{{ entity.idCalendario }}</td>
            </tr>
            <tr>
                <th>Dia</th>
                <td>{{ entity.dia|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Hora</th>
                <td>{{ entity.hora }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ entity.estado }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('calendarioevento') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('calendarioevento_edit', { 'id': entity.idCalendario }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
