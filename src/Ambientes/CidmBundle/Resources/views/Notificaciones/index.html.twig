{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Notificaciones list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Idnotificacion</th>
                <th>Idusuariogen</th>
                <th>Idusuarioreem</th>
                <th>Estado</th>
                <th>Descripcion</th>
                <th>Fechanotificacion</th>
                <th>Fecharespuesta</th>
                <th>Fechainicio</th>
                <th>Fechafin</th>
                <th>Horainicio</th>
                <th>Horafin</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('notificaciones_show', { 'id': entity.idNotificacion }) }}">{{ entity.idNotificacion }}</a></td>
                <td>{{ entity.idUsuarioGen }}</td>
                <td>{{ entity.idUsuarioReem }}</td>
                <td>{{ entity.estado }}</td>
                <td>{{ entity.descripcion }}</td>
                <td>{% if entity.fechaNotificacion %}{{ entity.fechaNotificacion|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.fechaRespuesta %}{{ entity.fechaRespuesta|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.fechaInicio %}{{ entity.fechaInicio|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.fechaFin %}{{ entity.fechaFin|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.horaInicio }}</td>
                <td>{{ entity.horaFin }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('notificaciones_show', { 'id': entity.idNotificacion }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('notificaciones_edit', { 'id': entity.idNotificacion }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('notificaciones_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
