{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Solicitud</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Idsolicitud</th>
                <td>{{ entity.idSolicitud }}</td>
            </tr>
            <tr>
                <th>Fechasolicitud</th>
                <td>{{ entity.fechaSolicitud|date('Y-m-d') }}</td>
            </tr>
            <tr>
                <th>Fecharespuesta</th>
                <td>{{ entity.fechaRespuesta|date('Y-m-d') }}</td>
            </tr>
            <tr>
                <th>Fechainicio</th>
                <td>{{ entity.fechaInicio|date('Y-m-d') }}</td>
            </tr>
            <tr>
                <th>Fechafin</th>
                <td>{{ entity.fechaFin|date('Y-m-d') }}</td>
            </tr>
            <tr>
                <th>Horainicio</th>
                <td>{{ entity.horaInicio|date('H:i:s') }}</td>
            </tr>
            <tr>
                <th>Horafin</th>
                <td>{{ entity.horaFin|date('H:i:s') }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ entity.estado }}</td>
            </tr>
            <tr>
                <th>Numeropersonas</th>
                <td>{{ entity.numeroPersonas }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('solicitud') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('solicitud_edit', { 'id': entity.idSolicitud }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
