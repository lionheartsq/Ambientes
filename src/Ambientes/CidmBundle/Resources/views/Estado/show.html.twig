{% extends '::base.html.twig' %}

{% block body -%}
  <div class="panel panel-default">
  <!-- Default panel contents -->
  <div class="panel-heading"><h1>Estado</h1></div>
    
    <table class="record_properties">
        <tbody>
            <tr>
                <th>Idestado</th>
                <td>{{ entity.idEstado }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ entity.descripcion }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('estado') }}">
            Regresar a la lista
        </a>
    </li>
    <li>
        <a href="{{ path('estado_edit', { 'id': entity.idEstado }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
  </div>
{% endblock %}
