{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Acceso list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Idacceso</th>
                <th>Contrasena</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('acceso_show', { 'id': entity.idAcceso }) }}">{{ entity.idAcceso }}</a></td>
                <td>{{ entity.contrasena }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('acceso_show', { 'id': entity.idAcceso }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('acceso_edit', { 'id': entity.idAcceso }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('acceso_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
